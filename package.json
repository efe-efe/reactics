{
    "name": "reactics",
    "private": true,
    "scripts": {
        "postinstall": "node scripts/install.js",
        "launch": "node scripts/launch.js",
        "build": "run-p build:*",
        "build:panorama": "node --preserve-symlinks node_modules/webpack/bin/webpack.js --config src/panorama/webpack.config.js",
        "build:vscripts": "tstl --project src/vscripts/tsconfig.json",
        "dev": "run-p dev:*",
        "dev:panorama": "node --preserve-symlinks node_modules/webpack/bin/webpack.js --config src/client/panorama.config.js --watch",
        "dev:web": "webpack-dev-server --open --config src/client/web.config.js --mode development",
        "dev:vscripts": "tstl --project src/vscripts/tsconfig.json --watch",
        "dev:server": "nodemon src/server/app.js",
        "lint": "eslint . --ext .ts --max-warnings 0"
    },
    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@babel/preset-env": "^7.19.4",
        "@babel/preset-react": "^7.18.6",
        "@types/react-dom": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.8.1",
        "@typescript-eslint/parser": "^5.8.1",
        "babel-loader": "^8.2.5",
        "css-loader": "6.5.1",
        "eslint": "^8.6.0",
        "eslint-config-prettier": "^8.3.0",
        "file-loader": "6.2.0",
        "fork-ts-checker-webpack-plugin": "^7.2.1",
        "nodemon": "^2.0.20",
        "terser-webpack-plugin": "^5.3.1",
        "ts-loader": "^9.2.5",
        "typescript": "^4.5.5",
        "typescript-to-lua": "^1.0.0",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.11.1"
    },
    "dependencies": {
        "@moddota/dota-lua-types": "^4.13.0",
        "@moddota/find-steam-app": "^1.1.0",
        "@moddota/panorama-types": "^1.12.0",
        "@reduxjs/toolkit": "^1.7.2",
        "@types/react": "17.0.19",
        "cors": "^2.8.5",
        "express": "^4.17.3",
        "fs-extra": "^9.0.0",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.5.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-panorama": "0.1.2",
        "react-redux": "^7.2.6"
    }
}
